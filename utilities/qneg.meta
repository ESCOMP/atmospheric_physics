[ccpp-table-properties]
  name = qneg
  type = scheme

[ccpp-arg-table]
  name  = qneg_init
  type  = scheme
[ print_qneg_warn ]
  standard_name = print_qneg_warn
  units = 1
  type = character | kind = len=*
  dimensions = ()
  intent = in
[ number_of_constituents ]
  standard_name = number_of_constituents
  units = count
  type = integer
  dimensions = ()
  intent = in
[ qmin ]
