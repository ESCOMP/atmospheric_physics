  units = count
  type = integer
  dimensions = ()
  intent = in
[ lver ]
  standard_name = vertical_layer_dimension
  units = count
  type = integer
  dimensions = ()
  intent = in
[ qmin ]
  standard_name = constituent_minimum_value
  units = 1
  type = real | kind = r8
