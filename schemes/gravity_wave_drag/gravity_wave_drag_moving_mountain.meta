[ccpp-table-properties]
  name = gravity_wave_drag_moving_mountain
  type = scheme
  dependencies = gw_utils.F90

[ccpp-arg-table]
  name  = gravity_wave_drag_moving_mountain_init
  type  = scheme
[ pver ]
  standard_name = vertical_layer_dimension
  units = count
  type = integer
  dimensions = ()
  intent = in
[ masterproc ]
  standard_name = flag_for_mpi_root
  units = flag
  type = logical
  dimensions = ()
  intent = in
[ iulog ]
  standard_name = log_output_unit
  units = 1
  type = integer
  dimensions = ()
  intent = in
[ file_path ]
  # gw_drag_file_mm
  standard_name = filename_of_lookup_table_of_source_spectra_for_moving_mountain_gravity_wave_drag
  units = none
  type = character | kind = len=*
  dimensions = ()
  intent = in
[ gw_delta_c ]
  standard_name = delta_between_nearest_phase_speeds_in_the_mid_gravity_wave_band_tbd
  units = m s-1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ pref_edge ]
  standard_name = reference_pressure_at_interface
  units = Pa
  type = real | kind = kind_phys
  dimensions = (vertical_interface_dimension)
  intent = in
[ movmtn_psteer ]
  standard_name = pressure_level_for_steering_in_moving_mountain_gravity_wave_drag_tbd
  units = Pa
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ movmtn_plaunch ]
  standard_name = pressure_level_for_launch_in_moving_mountain_gravity_wave_drag_tbd
  units = Pa
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ movmtn_source_nl ]
  standard_name = source_type_for_moving_mountain_gravity_wave_drag_tbd
  units = none
  type = integer
  dimensions = ()
  intent = in
[ errmsg ]
  standard_name = ccpp_error_message
  units = none
  type = character | kind = len=512
  dimensions = ()
  intent = out
[ errflg ]
  standard_name = ccpp_error_code
  units = 1
  type = integer
  dimensions = ()
  intent = out

[ccpp-arg-table]
