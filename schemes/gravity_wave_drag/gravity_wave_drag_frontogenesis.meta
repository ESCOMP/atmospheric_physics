[ccpp-table-properties]
  name = gravity_wave_drag_frontogenesis
  type = scheme
  dependencies = gw_utils.F90,gw_common.F90,../../to_be_ccppized/coords_1d.F90

[ccpp-arg-table]
  name  = gravity_wave_drag_frontogenesis_init
  type  = scheme
[ pver ]
  standard_name = vertical_layer_dimension
  units = count
  type = integer
  dimensions = ()
  intent = in
[ pi ]
  standard_name = pi_constant
  units = 1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ masterproc ]
  standard_name = flag_for_mpi_root
  units = flag
  type = logical
  dimensions = ()
  intent = in
[ iulog ]
  standard_name = log_output_unit
  units = 1
  type = integer
  dimensions = ()
  intent = in
[ pref_edge ]
  standard_name = reference_pressure_at_interface
  units = Pa
  type = real | kind = kind_phys
  dimensions = (vertical_interface_dimension)
  intent = in
[ frontgfc ]
  standard_name = critical_threshold_for_frontogenesis_function_for_frontogenesis_gravity_wave_drag
  units = K2 m-2 s-1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ gw_delta_c ]
  standard_name = delta_between_nearest_phase_speeds_in_the_mid_gravity_wave_band
  units = m s-1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ pgwv ]
  standard_name = mid_gravity_wave_band_spectrum_dimension
  units = 1
  type = integer
  dimensions = ()
  intent = in
[ taubgnd ]
  standard_name = background_source_strength_for_frontogenesis_gravity_wave_drag
  units = N m-2
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ effgw_cm ]
  standard_name = frontogenesis_scheme_efficiency_of_gravity_wave_momentum_transfer
  units = 1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ front_gaussian_width ]
  standard_name = width_of_gaussian_of_stress_profile_for_frontogenesis_gravity_wave_drag
  units = m s-1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ errmsg ]
  standard_name = ccpp_error_message
  units = none
  type = character | kind = len=512
  dimensions = ()
  intent = out
[ errflg ]
  standard_name = ccpp_error_code
  units = 1
  type = integer
  dimensions = ()
  intent = out

