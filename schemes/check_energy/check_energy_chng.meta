  units = J m-2 s-1
  type = real | kind = kind_phys
  dimensions = (horizontal_dimension)
  intent = out
[ tend_tw_tnd ]
  standard_name = cumulative_total_water_boundary_flux
  units = kg m-2 s-1
  type = real | kind = kind_phys
  dimensions = (horizontal_dimension)
  intent = out
