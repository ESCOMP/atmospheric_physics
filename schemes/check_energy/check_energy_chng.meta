  units = m
  type = real | kind = kind_phys
  dimensions = (horizontal_dimension, vertical_layer_dimension)
  intent = out
[ count ]
  standard_name = number_of_values_with_significant_energy_or_water_imbalances
  units = count
  type = integer
  dimensions = ()
  intent = out
[ teout ]
  standard_name = vertically_integrated_total_energy_at_end_of_physics
  units = J m-2
  type = real | kind = kind_phys
  dimensions = (horizontal_dimension)
  intent = out
[ energy_formula_physics ]
  standard_name = total_energy_formula_for_physics
  units = 1
  type = integer
  dimensions = ()
  intent = in
[ energy_formula_dycore ]
  standard_name = total_energy_formula_for_dycore
  units = 1
  type = integer
  dimensions = ()
  intent = in
[ errmsg ]
  standard_name = ccpp_error_message
  units = none
  type = character | kind = len=512
  dimensions = ()
  intent = out
[ errflg ]
  standard_name = ccpp_error_code
  units = 1
  type = integer
  dimensions = ()
  intent = out

