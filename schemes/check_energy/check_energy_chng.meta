  standard_name = horizontal_dimension
  units = count
  type = integer
  dimensions = ()
  intent = in
[ pver ]
  standard_name = vertical_layer_dimension
  units = count
  type = integer
  dimensions = ()
  intent = in
[ pcnst ]
