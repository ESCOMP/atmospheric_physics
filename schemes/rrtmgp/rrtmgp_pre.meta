  type = integer
  dimensions = ()
  intent = in
[ irad_always ]
  standard_name = number_of_timesteps_to_force_radiation_calculation_after_initialization
  units = count
  type = integer
  dimensions = ()
  intent = in
[ offset ]
  standard_name = number_of_seconds_until_next_shortwave_radiation_timestep
  units = s
  type = integer
  dimensions = ()
  intent = out
[ idxday ]
  standard_name = daytime_points
  units = index
  type = integer
  dimensions = (horizontal_dimension)
  intent = out
[ nday ]
  standard_name = daytime_points_dimension
  units = count
  type = integer
  dimensions = ()
  intent = out
[ idxnite ]
  standard_name = nighttime_points
  units = index
  type = integer
  dimensions =  (horizontal_dimension)
  intent = out
[ nnite ]
  standard_name = nighttime_points_dimension
  units = count
  type = integer
  dimensions = ()
