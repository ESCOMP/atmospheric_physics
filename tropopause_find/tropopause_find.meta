[ccpp-table-properties]
  name = tropopause_find
  type = scheme

[ccpp-arg-table]
  name  = tropopause_find_init
  type  = scheme
[ cappa ]
  standard_name = ratio_of_dry_air_gas_constant_to_specific_heat_of_dry_air_at_constant_pressure
  units = 1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ rair ]
  standard_name = gas_constant_of_dry_air
  units = J kg-1 K-1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ gravit ]
  standard_name = standard_gravitational_acceleration
  units = m s-2
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ pi ]
  standard_name = pi_constant
  units = 1
  type = real | kind = kind_phys
  dimensions = ()
  intent = in
[ errmsg ]
  standard_name = ccpp_error_message
  units = none
  type = character | kind = len=512
  dimensions = ()
  intent = out
[ errflg ]
  standard_name = ccpp_error_code
  units = 1
  type = integer
  dimensions = ()
  intent = out

